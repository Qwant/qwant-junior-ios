<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
    <ShortName>Qwant Junior</ShortName>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="16" height="16">data:image/png;base64,</Image>
    <Url type="application/x-suggestions+json" method="GET" template="https://api.qwantjunior.com/api/suggest/">
        <Param name="q" value="{searchTerms}"/>
        <Param name="client" value="opensearch"/>
    </Url>
    <Url type="text/html" method="GET" template="https://www.qwantjunior.com/">
        <Param name="q" value="{searchTerms}"/>
        <Param name="client" value="qwantbrowser"/>
    </Url>
    <SearchForm>https://www.qwantjunior.com/</SearchForm>
</SearchPlugin>

